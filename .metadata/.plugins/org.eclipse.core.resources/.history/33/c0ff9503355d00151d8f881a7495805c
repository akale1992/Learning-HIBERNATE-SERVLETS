package com.jspiders.springlist;

public class Answer {
	private int ansno;
	private String answer;
	public Answer(int ansno, String answer) {
		// TODO Auto-generated constructor stub
		this.ansno=ansno;
		this.answer = answer;
	}
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "For answer no :"+ansno+"answer is  :"+answer;
	}

}
